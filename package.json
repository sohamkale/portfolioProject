    "@fortawesome/react-fontawesome": "^0.1.9",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "express": "^4.17.3",
    "mocha": "^6.2.3",
    "mongoose": "^5.9.10",
    "morgan": "^1.10.0",
    "nodemon": "^1.19.4",
    "react-bootstrap-icons": "^1.0.1-alpha3",
    "react-id-generator": "^3.0.0",
    "should": "^13.2.3",
